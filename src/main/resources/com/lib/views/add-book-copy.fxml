<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<AnchorPane prefHeight="385.0" prefWidth="435.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.lib.controllers.AddBookCopyController">
    <children>
        <VBox prefHeight="385.0" prefWidth="435.0" spacing="20.0">
            <padding>
                <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
            </padding>
            <children>
                <TextField fx:id="addCopyTxtField" promptText="Enter book ISBN" styleClass="textField"/>
                <Label fx:id="addCopyLabel" prefHeight="34.0" prefWidth="393.0"/>
                <Button fx:id="addCopyBtn" mnemonicParsing="false" onAction="#addBookCopy" prefHeight="25.0"
                        prefWidth="409.0" styleClass="btn"
                        text="Add Copy"/>
            </children>
        </VBox>
    </children>
</AnchorPane>
